<%= render "admin/shared/bread_crumb", name: {title: "Edit welcome", bread_crumb: "Edit"} %>
<%= form_with model: @welcome, url: admin_welcome_path(@welcome), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @welcome, title_name: "Update welcome" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_welcome_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_welcome" %>
    </div>
  </section>
<% end %>
