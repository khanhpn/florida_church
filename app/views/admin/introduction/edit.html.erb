<%= render "admin/shared/bread_crumb", name: {title: "Edit introduction", bread_crumb: "Edit"} %>
<%= form_with model: @introduction, url: admin_introduction_path(@introduction), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_introduction_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_introduction" %>
    </div>
  </section>
<% end %>
