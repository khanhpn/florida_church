<%= render "admin/shared/bread_crumb", name: {title: "Edit music ministry", bread_crumb: "Edit"} %>
<%= form_with model: @music_ministry, url: admin_music_ministry_path(@music_ministry), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @music_ministry, title_name: "Update music ministry" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_music_ministry_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
