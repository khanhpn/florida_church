<%= render "admin/shared/bread_crumb", name: {title: "Edit engraved brick", bread_crumb: "Edit"} %>
<%= form_with model: @pray_garden, url: admin_pray_garden_path(@pray_garden), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_pray_garden_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_pray_garden" %>
    </div>
  </section>
<% end %>
