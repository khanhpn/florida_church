<%= render "admin/shared/bread_crumb", name: {title: "Edit google map", bread_crumb: "Edit"} %>
<%= form_with model: @google_map, url: admin_google_map_path(@google_map), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_google_map_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_google_map" %>
    </div>
  </section>
<% end %>
