<%= render "admin/shared/bread_crumb", name: {title: "Edit memorial book", bread_crumb: "Edit"} %>
<%= form_with model: @memorial_book, url: admin_memorial_book_path(@memorial_book), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @memorial_book, title_name: "Update memorial book" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_memorial_book_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
