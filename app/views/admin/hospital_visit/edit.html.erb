<%= render "admin/shared/bread_crumb", name: {title: "Edit hospital visit", bread_crumb: "Edit"} %>
<%= form_with model: @hospital_visit, url: admin_hospital_visit_path(@hospital_visit), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @hospital_visit, title_name: "Update hospital visit" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_hospital_visit_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
