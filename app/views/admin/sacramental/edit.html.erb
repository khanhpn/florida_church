<%= render "admin/shared/bread_crumb", name: {title: "Edit sacramental", bread_crumb: "Edit"} %>
<%= form_with model: @sacramental, url: admin_sacramental_path(@sacramental), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_sacramental_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_sacramental" %>
    </div>
  </section>
<% end %>
