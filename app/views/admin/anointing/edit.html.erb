<%= render "admin/shared/bread_crumb", name: {title: "Edit anoiting", bread_crumb: "Edit"} %>
<%= form_with model: @anointing, url: admin_anointing_path(@anointing), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @anointing, title_name: "Update anointing" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_anointing_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
