<%= render "admin/shared/bread_crumb", name: {title: "Edit confirmation", bread_crumb: "Edit"} %>
<%= form_with model: @confirmation, url: admin_confirmation_path(@confirmation), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @confirmation, title_name: "Update confirmation" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_confirmation_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
