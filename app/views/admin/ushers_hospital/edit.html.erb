<%= render "admin/shared/bread_crumb", name: {title: "Edit usher hospitals", bread_crumb: "Edit"} %>
<%= form_with model: @usher_hospital, url: admin_ushers_hospital_path(@usher_hospital), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_ushers_hospital_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_usher_hospital" %>
    </div>
  </section>
<% end %>
