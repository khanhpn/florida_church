<%= render "admin/shared/bread_crumb", name: {title: "Edit adult", bread_crumb: "Edit"} %>
<%= form_with model: @adult, url: admin_adult_path(@adult), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @adult, title_name: "Update adult" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_adult_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_adult" %>
    </div>
  </section>
<% end %>
