<%= render "admin/shared/bread_crumb", name: {title: "Edit history church", bread_crumb: "Edit"} %>
<%= form_with model: @history_church, url: admin_history_church_path(@history_church), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @history_church, title_name: "Update history church" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_history_church_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
