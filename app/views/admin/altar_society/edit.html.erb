<%= render "admin/shared/bread_crumb", name: {title: "Edit altar society", bread_crumb: "Edit"} %>
<%= form_with model: @altar_society, url: admin_altar_society_path(@altar_society), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_altar_society_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_altar_society" %>
    </div>
  </section>
<% end %>
