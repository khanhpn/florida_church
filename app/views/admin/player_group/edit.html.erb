<%= render "admin/shared/bread_crumb", name: {title: "Edit player group", bread_crumb: "Edit"} %>
<%= form_with model: @player_group, url: admin_player_group_path(@player_group), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @player_group, title_name: "Update player group" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_player_group_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
