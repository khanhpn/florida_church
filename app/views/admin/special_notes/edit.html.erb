<%= render "admin/shared/bread_crumb", name: {title: "Edit special note", bread_crumb: "Edit"} %>
<%= form_with model: @special_note, url: admin_special_note_path(@special_note), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_special_note_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_special_note" %>
    </div>
  </section>
<% end %>
