<%= render "admin/shared/bread_crumb", name: {title: "Edit holyorder", bread_crumb: "Edit"} %>
<%= form_with model: @holyorder, url: admin_holyorder_path(@holyorder), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @holyorder, title_name: "Update holyorder" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_holyorder_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
