<%= render "admin/shared/bread_crumb", name: {title: "Create gallery image", bread_crumb: "New"} %>
<%= form_with model: @gallery_image, url: admin_gallery_images_path do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_gallery_images_path, class: "btn btn-info" %>
      <%= form.submit "Create", class: "btn btn-success", id: "id_submit_gallery_image" %>
    </div>
  </section>
<% end %>
