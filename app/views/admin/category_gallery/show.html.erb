<%= render "admin/shared/bread_crumb", name: {title: "Show category gallery", bread_crumb: "Show"} %>
<section class="content">
  <div class="container-fluid">
    <table id="example2" class="table table-bordered table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= @category_gallery&.name || "-" %></td>
          <td>
            <% unless @category_gallery.blank? %>
              <%= link_to "Edit", edit_admin_category_gallery_path(@category_gallery), class: "btn btn-success" %>
              <%= link_to "Delete", admin_category_gallery_path(@category_gallery), method: :delete, class: "btn btn-danger" %>
              <%= link_to "Back", admin_category_gallery_index_path, class: "btn btn-primary" %>
            <% end %>
          </td>
        </tr>
      </tbody>
    </table>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</section>
