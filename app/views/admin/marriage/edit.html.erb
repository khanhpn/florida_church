<%= render "admin/shared/bread_crumb", name: {title: "Edit marriage", bread_crumb: "Edit"} %>
<%= form_with model: @marriage, url: admin_marriage_path(@marriage), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_marriage_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_marriage" %>
    </div>
  </section>
<% end %>
