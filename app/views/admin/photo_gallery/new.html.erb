<%= render "admin/shared/bread_crumb", name: {title: "Create photo gallery", bread_crumb: "New"} %>
<%= form_with model: @photo_gallery, url: admin_category_gallery_photo_gallery_index_path(params[:category_gallery_id]) do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_category_gallery_path(params[:category_gallery_id]), class: "btn btn-info" %>
      <%= form.submit "Create", class: "btn btn-success", id: "id_submit_gallery_image" %>
    </div>
  </section>
<% end %>
