<%= render "admin/shared/bread_crumb", name: {title: "Edit teaching mass", bread_crumb: "Edit"} %>
<%= form_with model: @teaching_mass, url: admin_teaching_mass_path(@teaching_mass), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @teaching_mass, title_name: "Update teaching mass" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_teaching_mass_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_teaching_mass" %>
    </div>
  </section>
<% end %>
