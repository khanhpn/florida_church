<%= render "admin/shared/bread_crumb", name: {title: "Edit food basket", bread_crumb: "Edit"} %>
<%= form_with model: @food_basket, url: admin_food_basket_path(@food_basket), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @food_basket, title_name: "Update food basket" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_food_basket_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
