<%= render "admin/shared/bread_crumb", name: {title: "Edit clergy", bread_crumb: "Edit"} %>
<%= form_with model: @clergy, url: admin_clergy_path(@clergy), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @clergy, title_name: "Update clergy" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_clergy_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
