<%= render "admin/shared/bread_crumb", name: {title: "Edit children youth", bread_crumb: "Edit"} %>
<%= form_with model: @children_youth, url: admin_children_youth_path(@children_youth), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @children_youth, title_name: "Update children youth" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_children_youth_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
