<%= render "admin/shared/bread_crumb", name: {title: "Edit youth", bread_crumb: "Edit"} %>
<%= form_with model: @youth, url: admin_youth_path(@youth), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @youth, title_name: "Update youth" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_youth_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_youth" %>
    </div>
  </section>
<% end %>
