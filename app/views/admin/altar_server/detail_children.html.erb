<%= render "admin/shared/bread_crumb", name: {title: "Detail altar server children", bread_crumb: "Update"} %>
<%= form_with model: @detail_children, url: altar_server_update_children_admin_altar_server_path(@altar_server, @detail_children), method: :put do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form_children", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_altar_server_path(@altar_server), class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit_altar_server" %>
    </div>
  </section>
<% end %>
