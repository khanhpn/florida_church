<%= render "admin/shared/bread_crumb", name: {title: "Edit altar server", bread_crumb: "Edit"} %>
<%= form_with model: @altar_server, url: admin_altar_server_path(@altar_server), method: :patch do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @altar_server, title_name: "Update altar server" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_altar_server_index_path, class: "btn btn-info" %>
      <%= form.submit "Update", class: "btn btn-success", id: "id_submit" %>
    </div>
  </section>
<% end %>
