<%= render "admin/shared/bread_crumb", name: {title: "Create altar server", bread_crumb: "New"} %>
<%= form_with model: @altar_server, url: admin_altar_server_index_path do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "admin/shared_form/form", form: form, item: @altar_server, title_name: "Create altar server" %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_altar_server_index_path, class: "btn btn-info" %>
      <%= form.submit "Create", class: "btn btn-success", id: "id_submit_altar_server" %>
    </div>
  </section>
<% end %>
