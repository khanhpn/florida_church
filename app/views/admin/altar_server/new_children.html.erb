<%= render "admin/shared/bread_crumb", name: {title: "Create altar server children", bread_crumb: "Create"} %>
<%= form_with model: @worship, url: children_admin_altar_server_path(@altar_server, @worship), method: :post do |form| %>
  <section class="content">
    <div class="container-fluid">
      <%= render "form_children", form: form %>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
    <div class="section-btn-group-wrap">
      <%= link_to "Back", admin_altar_server_path(@altar_server), class: "btn btn-info" %>
      <%= form.submit "Create", class: "btn btn-success", id: "id_submit_altar_server" %>
    </div>
  </section>
<% end %>
